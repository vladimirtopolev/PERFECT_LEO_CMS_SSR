  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "noImplicitAny": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "lib": [
      "es2017",
      "dom"
    ],
    "jsx": "react",
    "typeRoots": [
      "../core/web/@types",
      "../../core/web/@types",
      "../node_modules/@types"],
    "resolveJsonModule": true
  }
}
